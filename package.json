{
  "name": "ngsharsha",
  "version": "1.0.0",
  "description": "portfolio website",
  "main": "index.js",
  "repository": "https://github.com/HarshaVardhanNakkina/ngsharsha",
  "author": "HarshaVardhanNakkina <harshavardhan.n@yahoo.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch:sass": "sass --watch src/sass:src/css",
    "watch:eleventy": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "build:sass": "cross-env NODE_ENV=production sass src/sass:src/css --no-source-map",
    "build:eleventy": "cross-env ELEVENTY_ENV=production eleventy",
    "start": "concurrently \"yarn:watch:sass\" \"yarn:watch:eleventy\"",
    "build": "concurrently \"yarn:build:sass\" \"yarn:build:eleventy\"",
    "postbuild": "postcss _site/css/*.css -u autoprefixer cssnano -r --no-map"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.6",
    "eleventy-plugin-lazyimages": "^2.1.0",
    "markdown-it": "^12.1.0",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "sass": "^1.35.2"
  }
}
